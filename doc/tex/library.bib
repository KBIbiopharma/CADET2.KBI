volume = {1183},
year = {2008}
}
@article{Nowakowski25C,
abstract = {A theory of non-linear chromatography based on the mass balance equation is presented. The model is a system of partial differential equations and it includes axial dispersion in the mobile phase, flow in the mobile phase and mass transfer between phases. In the case of the injection of a mixture competition between components for places in the stationary or the mobile phase is taken into account. In the paper are discussed: numerical calculations of concentration and volume overloading and the effect of the mutual influence of the components of the injected mixture on the process of mass transfer between phases. Additionally, results of experiments for convex and concave adsorption isotherms are presented. In both cases the results obtained qualitatively confirm the theoretical analyses.},
