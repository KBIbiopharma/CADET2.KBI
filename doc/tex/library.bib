journal = {Journal of Chromatography A},
keywords = { Onset of viscous fingering, Viscosity contrast, Viscous fingering,Band dispersion},
month = jun,
number = {2},
pages = {137--145},
title = {{Visualising the onset of viscous fingering in chromatography columns}},
