file = {:home/schnittert/docs/mendeley/auto/Staby et al. - 2004 - Comparison of chromatographic ion-exchange resins III. Strong cation-exchange resins.pdf:pdf},
issn = {0021-9673},
journal = {Journal of Chromatography A},
keywords = { Dynamic capacity, Ion-exchangers, LC, Proteins, Stationary phases,Static capacity},
month = apr,
