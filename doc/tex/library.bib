issn = {0021-9673},
journal = {Journal of Chromatography A},
keywords = { Peptides, Proteins, Proteomics, Sample prefractionation, Serial displacement chromatofocusing, Two-dimensional gel electrophoresis, Two-dimensional high-performance liquid chromatog,Chromatofocusing},
month = feb,
number = {6},
pages = {977--984},
title = {{Serial displacement chromatofocusing and its applications in multidimensional chromatography and gel electrophoresis: II. Experimental results}},
