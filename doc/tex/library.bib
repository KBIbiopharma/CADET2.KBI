pages = {109--119},
title = {{Model based robustness analysis of an ion-exchange chromatography step}},
url = {http://www.sciencedirect.com/science/article/B6TG8-4MFJJ91-1/2/054688c697851d55b2fa25fb8abd1a42},
volume = {1138},
year = {2007}
