@article{Khirevich.etal10JoCA,
abstract = {We quantified the microstructural disorder of packed beds and correlated it with the resulting eddy dispersion. For this purpose we designed a set of bulk (unconfined) monodisperse random sphere packings with a systematic, protocol-dependent degree of microstructural heterogeneity, covering a porosity range from the random-close to the random-loose packing limit ([var epsilon] = 0.366-0.46). With the precise knowledge of particle positions, size, and shape we conducted a Vorono\"{\i} tessellation of all packings and correlated the statistical moments of the Vorono\"{\i} volume distributions (standard deviation and skewness) with the porosity and the protocol-dependent microstructural disorder. The deviation of the Vorono\"{\i} volume distributions from the delta function of a crystalline packing describes the origin of short-range disorder of the investigated random packings. Eddy dispersion was simulated over a wide range of reduced velocities (0.5 <= [nu] <= 750) and analyzed with the comprehensive Giddings equation. Transient dispersion was found to correlate with the spatial scales of heterogeneity in the packings. The analysis of short-range disorder based on the Vorono\"{\i} volume distributions revealed a strong correlation with the short-range interchannel contribution to eddy dispersion, whereas transchannel dispersion was relatively little affected. The presented approach defines a strictly scientific route to the key morphology-transport relationships of current and future chromatographic supports, including their morphological reconstruction, statistical analysis, and the correlation with relevant transport phenomena. It also guides us in our understanding, comparison, and optimization of the diverse packing algorithms and protocols used in simulations and experimental studies.},
author = {Khirevich, Siarhei and Daneyko, Anton and H\"{o}ltzel, Alexandra and Seidel-Morgenstern, Andreas and Tallarek, Ulrich},
file = {:home/schnittert/docs/mendeley/auto/Khirevich et al. - 2010 - Statistical analysis of packed beds, the origin of short-range disorder, and its impact on eddy dispersion.pdf:pdf},
issn = {0021-9673},
journal = {Journal of Chromatography A},
keywords = { Degree of heterogeneity, Direct numerical simulation, Disorder, Eddy dispersion, High-performance computing, Lattice-Boltzmann method, Microstructure, Ordered packing, Packing method, Random packing, Statistical analysis, Vorono\"{\i} tessellation, Vorono\"{\i} volume distribution,Packed beds},
month = jul,
number = {28},
pages = {4713--4722},
title = {{Statistical analysis of packed beds, the origin of short-range disorder, and its impact on eddy dispersion}},
url = {http://www.sciencedirect.com/science/article/B6TG8-5041291-4/2/837bbd590a96e878541d1b0ccaf3e109},
volume = {1217},
year = {2010}
}
