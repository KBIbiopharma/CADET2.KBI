journal = {Journal of Chromatography A},
keywords = { Composite media, Ion exchange, Protein transport, Proteins,Confocal laser scanning microscopy},
month = dec,
number = {1-2},
pages = {105--115},
title = {{Mechanism and kinetics of protein transport in chromatographic media studied by confocal laser scanning microscopy: Part II. Impact on chromatographic separations}},
url = {http://www.sciencedirect.com/science/article/B6TG8-49NXH0X-B/2/611f833bd007995d07b809fb35a6527d},
