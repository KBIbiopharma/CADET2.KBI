journal = {Journal of Chromatography A},
keywords = { Ion exchange, LC, Preparative chromatography, Proteins, Static capacity, Stationary phases,Dynamic capacity},
month = sep,
number = {1-2},
pages = {82--94},
title = {{Comparison of chromatographic ion-exchange resins: VI. Weak anion-exchange resins}},
url = {http://www.sciencedirect.com/science/article/B6TG8-4P2S936-3/2/e76a5c242544e002616a95d505e70daf},
volume = {1164},
