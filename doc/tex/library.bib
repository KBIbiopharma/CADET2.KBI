file = {:home/schnittert/docs/mendeley/auto/Kaltenbrunner, Jungbauer - 1996 - Adsorption isotherms in protein chromatography combined influence of protein and salt concentration on adsorption isotherm.pdf:pdf},
issn = {0021-9673},
journal = {Journal of Chromatography A},
keywords = { Albumin, Distribution coefficients, Peak shape, Proteins,Adsorption isotherms},
month = may,
